                   {% for item in newsletter %}
					<div class="post">
						<div class="post-media post-image">
							<img src="{{item.thumbnail.src}}" class="img-responsive" alt="">
						</div>

						<div class="post-body">
							<div class="entry-header">
								<span class="post-meta-date"><i class="fa fa fa-clock-o"></i>{{item.get_date}}</span>
   							<h2 class="entry-title">
					 				<a href="#">{{item.title}}</a>
					 			</h2>
					 			<div class="post-meta">
									<span class="post-author">
   									Posted By <a href="#">{{item.get_author}}</a>
   								</span>
									{# <span class="post-cat">
   									Posted In: <a href="#">Insurance, Policy</a>
   								</span> #}
									{# <span class="post-comment">03<a href="#" class="comments-link">Comments</a></span> #}
								</div>
							</div><!-- header end -->

							<div class="entry-content">
								<p>{{item.get_content(30)}}</p>
							</div>

							<div class="post-footer">
								<a href="{{item.link}}" class="btn btn-primary">Continue Reading</a>
							</div>

						</div><!-- post-body end -->
					</div><!-- 3rd post end -->
